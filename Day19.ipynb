    "import re\n",
    "def solution(myStr):\n",
    "    answer = re.split(\"a|b|c\", myStr) #구분자가 여러개일때 re.split으로 | 이용\n",
    "    an=[]\n",
    "    for i in answer:\n",
    "        if i:\n",
    "            an.append(i)\n",
    "    if an == []:\n",
    "        an=[\"EMPTY\"]\n",
    "    return an"
   ]
  }
