    "def solution(intStrs, k, s, l):\n",
    "    answer = []\n",
    "    \n",
    "    for i in intStrs:\n",
    "        if int(i[s:s+l]) > k:\n",
    "            answer.append(int(i[s:s+l]))\n",
    "    \n",
    "    return answer"
   ]
