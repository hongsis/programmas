   "source": [
    "# 특정 문자열로 끝나는 가장 긴 부분 문자열 찾기\n",
    "\n",
    "def solution(myString, pat):\n",
    "    answer = ''\n",
    "    mystr=myString[::-1]\n",
    "    patt=pat[::-1]\n",
    "    if patt in mystr:\n",
    "        idx=mystr.index(patt)\n",
    "        answer = mystr[idx:][::-1]\n",
    "    return answer"
   ]
  }
