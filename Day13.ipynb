    "def solution(str_list):\n",
    "    for i in range(len(str_list)):\n",
    "        if str_list[i] == \"l\": # l이 나오면:\n",
    "            return str_list[0:i]\n",
    "        elif str_list[i] == \"r\": # r이 나오면:\n",
    "            return str_list[i+1:]\n",
    "\n",
    "    return []"
   ]
  }
