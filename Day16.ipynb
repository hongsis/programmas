    "def solution(myString):\n",
    "    answer = ''\n",
    "    for i in myString:\n",
    "        if i =='a':\n",
    "            answer+='A'\n",
    "        elif i !='A':\n",
    "            answer+=i.lower()\n",
    "        elif i =='A':\n",
    "            answer+=i\n",
    "    return answer\n",
    "\n",
    "# 모든 경우가 다 포함되도록 !"
   ]
  }
